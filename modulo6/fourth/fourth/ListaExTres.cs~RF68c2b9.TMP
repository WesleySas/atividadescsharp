using System;
using System.Collections.Generic;
using System.Text;

namespace fourth
{
    class ListaExTres
    {
        static void Main()
        {

            int[,] matriz = new int[3, 4];
            Console.WriteLine(matriz);

            for (int i = 0; i < 3; i++)
            {
                string[] v = Console.ReadLine().Split(" ");
                for(int j = 0; j < 4; j++)
                {
                    matriz[i, j] = int.Parse(v[j]);
                }
            }

            for (int i = 0; i < 3; i++)
            {
                for (int j = 0; j < 4; j++)
                {
                    Console.WriteLine(matriz[i,j]);
                }
            }
        }
    }
}
